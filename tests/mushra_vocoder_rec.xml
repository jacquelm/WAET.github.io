      <scales>
        <scalelabel position="0">Mauvais (0)</scalelabel>
        <scalelabel position="25">Faible (25)</scalelabel>
        <scalelabel position="50">Moyen (50)</scalelabel>
        <scalelabel position="75">Bon (75)</scalelabel>
        <scalelabel position="100">Excellent (100)</scalelabel>
      </scales>
    </interface>

